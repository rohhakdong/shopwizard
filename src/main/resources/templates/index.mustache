{{>layout/header}}
    <h1>스프링 부트로 시작하는 웹서비스</h1>
    <div class="col-md-12">
        <div class="row">
            <div class="col-md-6">
                <a href="/mantnc/save" role="button" class="btn btn-primary">글 등록</a>
            </div>
        </div>
    </div>
    <br>
    <table class="table table-horizontal table-bordered">
        <thead class="thead-strong">
        <tr>
            <th>번호</th>
            <th>제목</th>
            <th>작자</th>
            <th>최종수정</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#mantncList}}
            <tr>
                <td><a href="/mantnc/update/{{id}}">{{id}}</a></td>
                <td><a href="/mantnc/update/{{id}}">{{title}}</a></td>
                <td>{{author}}</td>
                <td>{{modifiedDate}}</td>
            </tr>
        {{/mantncList}}
        </tbody>
    </table>
{{>layout/footer}}
